<?xml version="1.0" encoding="utf-8"?>
<GridLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.usuario.inventory.DashboardActivity"
    android:rowCount="4"
    android:columnCount="5"
    android:orientation="vertical"
    >


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="102dp"
        android:layout_height="89dp"
        app:srcCompat="@drawable/chair"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="2dp"
        />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="105dp"
        android:layout_height="92dp"
        android:layout_column="1"
        android:layout_row="0"
        app:srcCompat="@drawable/inventory"
        />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="104dp"
        android:layout_height="107dp"
        android:layout_column="0"
        android:layout_row="2"
        app:srcCompat="@drawable/mouse"
        />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="108dp"
        android:layout_height="107dp"
        android:layout_column="1"
        android:layout_row="1"
        app:srcCompat="@drawable/keyboard"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="111dp"
        android:layout_height="93dp"
        android:layout_column="2"
        android:layout_row="1"
        app:srcCompat="@drawable/monitor"
        />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="73dp"
        android:layout_height="91dp"
        android:layout_column="2"
        android:layout_row="2"
        app:srcCompat="@drawable/proyector"
        />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="75dp"
        android:layout_height="86dp"
        android:layout_column="1"
        android:layout_row="2"
        app:srcCompat="@drawable/printer"
        />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="100dp"
        android:layout_height="113dp"
        android:layout_column="0"
        android:layout_row="3"
        app:srcCompat="@drawable/table"
        />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="90dp"
        android:layout_height="127dp"
        android:layout_column="1"
        android:layout_row="3"
        app:srcCompat="@drawable/whiteboard"
        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="125dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/closet"
        android:layout_row="1"
        android:layout_column="0"
        />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="141dp"
        android:layout_height="102dp"
        android:layout_column="2"
        android:layout_row="0"
        app:srcCompat="@drawable/cpu"
        />

</GridLayout>
